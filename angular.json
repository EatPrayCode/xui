{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "xui": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "changeDetection": "OnPush",
          "style": "scss"
        }
      },
      "root": "projects/xui",
      "sourceRoot": "projects/xui/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/xui",
            "index": "projects/xui/src/index.html",
            "main": "projects/xui/src/main.ts",
            "polyfills": "projects/xui/src/polyfills.ts",
            "tsConfig": "projects/xui/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/xui/src/favicon.ico",
              "projects/xui/src/assets"
            ],
            "styles": [
              "projects/xui/src/styles-app-loading.scss",
              "projects/xui/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/xui/src/environments/environment.ts",
                  "with": "projects/xui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2000kb"
                },
                {
                  "type": "bundle",
                  "name": "polyfills",
                  "baseline": "150kb",
                  "maximumWarning": "50kb",
                  "maximumError": "2000kb"
                },
                {
                  "type": "bundle",
                  "name": "styles",
                  "baseline": "280kb",
                  "maximumWarning": "50kb",
                  "maximumError": "2000kb"
                },
                {
                  "type": "bundle",
                  "name": "main",
                  "baseline": "1024kb",
                  "maximumWarning": "100kb",
                  "maximumError": "4000kb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "800kb"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "xui:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "xui:build:production"
            },
            "development": {
              "browserTarget": "xui:build:development"
            }
        },
        "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "xui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/xui/src/test.ts",
            "polyfills": "projects/xui/src/polyfills.ts",
            "tsConfig": "projects/xui/tsconfig.spec.json",
            "karmaConfig": "projects/xui/karma.conf.js",
            "assets": [
              "projects/xui/src/favicon.ico",
              "projects/xui/src/assets"
            ],
            "styles": [
              "projects/xui/src/styles-app-loading.scss",
              "projects/xui/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "test": {
              "fileReplacements": [
                {
                  "replace": "projects/xui/src/environments/environment.ts",
                  "with": "projects/xui/src/environments/environment.test.ts"
                }
              ]
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/xui/tsconfig.app.json",
              "projects/xui/tsconfig.spec.json",
              "projects/xui/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/xui/e2e/protractor.conf.js",
            "devServerTarget": "xui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "xui:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "xui",
  "cli": {
    "analytics": false
  }
}